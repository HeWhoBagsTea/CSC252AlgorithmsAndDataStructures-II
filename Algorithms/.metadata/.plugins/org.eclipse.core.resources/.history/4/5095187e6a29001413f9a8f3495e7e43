public class Driver {

	public static void main(String[] args) {		
		AVLBasedPriorityQueue<Integer> avl = new AVLBasedPriorityQueue<Integer>();
		HeapBasedPriorityQueue<Integer> heap = new HeapBasedPriorityQueue<Integer>();
		
		
	//---------------Heap Test----------------\\
	heap.offer(13);
	heap.offer(15);
	heap.offer(20);
	heap.offer(5);
	heap.offer(7);
	heap.offer(11);
	heap.offer(23);

	heap.printList();
	System.out.println("---Peek test--- \nFirst peek: " + heap.peek() + "\nSecond peek: " + heap.peek());
	System.out.println("---Poll test--- \nFirst poll: " + heap.poll() + " \nNew heap array: ");
	heap.printList();
	System.out.println("Second poll: " + heap.poll() + " \nNew heap array: ");
	heap.printList();
		
	//----------------AVL Test----------------\\
		
		
		
		
		
	}

}
