
public class Driver {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String str1 = "heeeelooaaaaymndddccxiii";
		String str2 = "hello";
		byte[] strBa = str1.getBytes();
		byte[] ba = new byte[] { 45, 56, 67, 78, 89, 12, 23, 34, 45, 23, 45, 67, 45 };
		byte[] test = str2.getBytes();
		HuffmanTree ht = new HuffmanTree(strBa);
//		ht.printNodes();
		
		Bits bits = new Bits();
//		bits.add(true);
//		bits.add(true);
//		bits.add(true);
//		bits.add(false);
//		System.out.println(ht.toByte(bits));
//		ht.fromByte(bit, bits);
		
		HuffmanCompressor hc = new HuffmanCompressor();
		byte[] compressed = hc.compress(ht, test);
		
//		System.out.println();
		for(int i = 0; i < compressed.length; i++)
			System.out.print(compressed[i] + " ");
		
		
		System.out.println();
		byte[] decompressed = hc.decompress(ht, 19, compressed);
		for(int i = 0; i < decompressed.length; i++)
			System.out.print(decompressed[i] + " ");
		
		char[] ascii = hc.convertAscii(decompressed);
		for(int i = 0; i < ascii.length; i++)
			System.out.print(ascii[i]);
	}

}
